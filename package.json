{
  "name": "template",
  "version": "0.0.1",
  "description": "Template server in NestJS with GraphQL, TypeORM and PostgreSQL using Fastify",
  "author": "Petar VujoviÄ‡",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:hot": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "build:image": "docker build . -t vegelibri-backend:latest",
    "compose:dev": "docker-compose up --force-recreate --scale template-prod=0",
    "compose:prod": "docker-compose up --force-recreate --scale template-dev=0",
    "start": "nest start",
    "start:hot": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "fix": "yarn lint && yarn format",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:gen": "yarn typeorm migration:generate -n",
    "db:create": "yarn typeorm migration:create -n",
    "db:migrate": "yarn typeorm migration:run",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.5",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-fastify": "7.3",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-fastify": "^2.19.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dataloader": "^2.0.0",
    "fastify-compress": "^2.0.0",
    "fastify-cookie": "^3.5",
    "fastify-cors": "^3.0.2",
    "fastify-csrf": "^3",
    "fastify-helmet": "^3.0.2",
    "fastify-rate-limit": "^3",
    "graphql": "^15.4.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^0.7.2",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^7.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pino-pretty": "^4.3.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.5",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2",
    "webpack-node-externals": "^2.5.2"
  }
}
